{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "windows-server/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.json",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "windows-server/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/windows-server/breadcrumbs/toc.json",
      "extendBreadcrumb": true,
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "WS.WindowsServerDocs"
        }
      }
    },
    "fileMetadata": {
      "feedback_product_url": {
        "manage/windows-admin-center/*.md": "http://aka.ms/wacfeedback",
        "manage/windows-admin-center/**/*.md": "http://aka.ms/wacfeedback" 
      },
       "open_to_public_contributors": {
         "storage/disk-management/**.md": true,
         "remote/remote-desktop-services/**.md": true,
         "manage/windows-admin-center/**.md": true,
         "manage/windows-admin-center/understand/**.md": true,
         "security/windows-services/**.md": true,
         "get-started/**.md": true
       }
    },
    "template": [
      null
    ],
    "dest": "windows-server"
  }
}
